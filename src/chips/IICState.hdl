  Mux(a=b8, b=false, sel=reset, out=in9);
  Bit(in=in9, load=load, out=b9);
  Copy(in=b9, out=out[9]);

  Mux(a=b9, b=false, sel=reset, out=in10);
  Bit(in=in10, load=load, out=b10);
  Copy(in=b10, out=out[10]);

  Mux(a=b10, b=false, sel=reset, out=in11);
  Bit(in=in11, load=load, out=b11);
  Copy(in=b11, out=out[11]);
  Mux(a=b11, b=false, sel=reset, out=in12);
  Bit(in=in12, load=load, out=b12);
  Copy(in=b12, out=out[12]);
  Mux(a=b12, b=false, sel=reset, out=in13);
  Bit(in=in13, load=load, out=b13);
  Copy(in=b13, out=out[13]);
  Mux(a=b13, b=false, sel=reset, out=in14);
  Bit(in=in14, load=load, out=b14);
  Copy(in=b14, out=out[14]);
  Mux(a=b14, b=false, sel=reset, out=in15);
  Bit(in=in15, load=load, out=b15);
  Copy(in=b15, out=out[15]);
}
