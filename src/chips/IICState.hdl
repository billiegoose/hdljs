CHIP IICState {
  IN inc, reset;
  OUT out[16];

  PARTS:
  Or(a=inc, b=reset, out=load);
  Mux(a=false, b=true, sel=reset, out=in0);
  Bit(in=in0, load=load, out=b0);
  Copy(in=b0, out=out[0]);
  Mux(a=b0, b=false, sel=reset, out=in1);
  Bit(in=in1, load=load, out=b1);
  Copy(in=b1, out=out[1]);
  Mux(a=b1, b=false, sel=reset, out=in2);
  Bit(in=in2, load=load, out=b2);
  Copy(in=b2, out=out[2]);
  Mux(a=b2, b=false, sel=reset, out=in3);
  Bit(in=in3, load=load, out=b3);
  Copy(in=b3, out=out[3]);
  Mux(a=b3, b=false, sel=reset, out=in4);
  Bit(in=in4, load=load, out=b4);
  Copy(in=b4, out=out[4]);
  Mux(a=b4, b=false, sel=reset, out=in5);
  Bit(in=in5, load=load, out=b5);
  Copy(in=b5, out=out[5]);
  Mux(a=b5, b=false, sel=reset, out=in6);
  Bit(in=in6, load=load, out=b6);
  Copy(in=b6, out=out[6]);
  Mux(a=b6, b=false, sel=reset, out=in7);
  Bit(in=in7, load=load, out=b7);
  Copy(in=b7, out=out[7]);
  Mux(a=b7, b=false, sel=reset, out=in8);
  Bit(in=in8, load=load, out=b8);
